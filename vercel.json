{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/app/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/chat/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "api/index.py"
    }
  ]
}

